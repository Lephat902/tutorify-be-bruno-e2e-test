meta {
  name: 2nd tutor approve his own application
  type: http
  seq: 12
}

patch {
  url: {{base_url}}/classes/applications/{{2nd_tutor_application_id}}/approve
  body: none
  auth: bearer
}

auth:bearer {
  token: {{2nd_tutor_access_token}}
}

assert {
  res.status: eq 400
  res.responseTime: lte {{medium_response_time}}
  res.body: length 1
  res.body: contains There is something wrong with your request
}
