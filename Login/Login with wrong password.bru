meta {
  name: Login with wrong password
  type: http
  seq: 3
}

post {
  url: {{base_url}}/login
  body: json
  auth: none
}

body:json {
  {
      "email": "{{sample_student_email}}",
      "password": "arbitrary text"
  }
}

assert {
  res.status: eq 401
  res.body: length 1
  res.body[0]: matches ^Invalid credentials! You have \d+ login attempts left!$
  res.responseTime: lte {{high_response_time}}
}
