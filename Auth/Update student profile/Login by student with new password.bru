meta {
  name: Login by student with new password
  type: http
  seq: 4
}

post {
  url: {{base_url}}/login
  body: json
  auth: none
}

body:json {
  {
      "email": "{{sample_student_email}}",
      "password": "newpassword"
  }
}

assert {
  res.status: eq 201
}

script:post-response {
  bru.setVar("access_token", res.body.token);
}
