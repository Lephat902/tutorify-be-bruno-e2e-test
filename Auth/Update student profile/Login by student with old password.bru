meta {
  name: Login by student with old password
  type: http
  seq: 8
}

post {
  url: {{base_url}}/login
  body: json
  auth: none
}

body:json {
  {
      "email": "{{sample_student_email}}",
      "password": "{{sample_student_password}}"
  }
}

assert {
  res.status: eq 201
  res.body.user.avatar: isNull
}

script:post-response {
  bru.setVar("access_token", res.body.token);
}
