meta {
  name: Fetch current tutor profile
  type: graphql
  seq: 2
}

post {
  url: {{graphql_base_url}}
  body: graphql
  auth: inherit
}

body:graphql {
  query Query {
    myProfile {
      ... on Tutor {
        address
        gender
        avatar {
          id
        }
        proficiencies {
          id
        }
        socialProfiles {
          id
        }
        tutorPortfolios {
          id
        }
      }
    }
  }
}

assert {
  res.body.data.myProfile.address: isNull
  res.status: eq 200
  res.body.data.myProfile.gender: isNull
  res.body.data.myProfile.avatar: isNull
  res.body.data.myProfile.proficiencies: length 0
  res.body.data.myProfile.socialProfiles: length 0
  res.body.data.myProfile.tutorPortfolios: length 0
}
