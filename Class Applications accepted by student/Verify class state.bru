meta {
  name: Verify class state
  type: graphql
  seq: 10
}

post {
  url: {{graphql_base_url}}
  body: graphql
  auth: none
}

body:graphql {
  query ExampleQuery($classId: String!) {
    class(id: $classId) {
      id
      status
      tutorId
    }
  }
  
}

body:graphql:vars {
  {
    "classId": "{{created_class_id}}"
  }
}

assert {
  res.body.data.class.id: eq {{created_class_id}}
  res.body.data.class.status: eq ASSIGNED
  res.body.data.class.tutorId: eq {{sample_tutor_id_1}}
}
