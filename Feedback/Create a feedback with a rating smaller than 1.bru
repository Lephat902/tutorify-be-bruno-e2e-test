meta {
  name: Create a feedback with a rating smaller than 1
  type: http
  seq: 8
}

post {
  url: {{base_url}}/tutors/{{sample_tutor_id}}/feedbacks
  body: json
  auth: inherit
}

body:json {
  {
    "text": "This is the content of the feedback",
    "rate": 0
  }
}

assert {
  res.status: eq 400
  res.responseTime: lte {{medium_response_time}}
  res.body: contains rate must not be less than 1
}
