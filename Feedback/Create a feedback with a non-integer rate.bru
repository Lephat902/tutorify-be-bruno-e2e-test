meta {
  name: Create a feedback with a non-integer rate
  type: http
  seq: 5
}

get {
  url: {{base_url}}/tutors/{{sample_tutor_id}}/feedbacks
  body: json
  auth: inherit
}

body:json {
  {
    "text": "This is the content of the feedback",
    "rate": 4.5
  }
}

assert {
  res.status: eq 400
  res.responseTime: lte {{medium_response_time}}
  res.body: contains rate must be a number conforming to the specified constraints
}
